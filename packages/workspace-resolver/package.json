{
  "name": "@modular-scripts/workspace-resolver",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "dependencies": {
    "fs-extra": "^10.1.0",
    "@esm2cjs/globby": "13.1.4",
    "parse-gitignore": "^1.0.1",
    "semver": "7.3.7"
  },
  "devDependencies": {
