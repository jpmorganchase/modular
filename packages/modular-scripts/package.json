    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/lockfile": "^1.0.1",
    "babel-jest": "^26.6.3",
    "babel-preset-react-app": "^10.0.0",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.1.1",
    "change-case": "^4.1.2",
    "commander": "^7.2.0",
    "esbuild": "^0.12.9",
    "esbuild-loader": "^2.13.0",
    "execa": "^5.1.1",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "is-ci": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.3.5",
    "prompts": "^2.4.1",
    "react-native-web": "^0.16.5",
    "react-scripts": "^4.0.3",
    "resolve": "^1.20.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "ts-jest": "^26.5.6",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@schemastore/package": "^0.0.6",
    "@schemastore/tsconfig": "^0.0.9",
    "@types/find-up": "^4.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/is-ci": "^3.0.0",
    "@types/node": "*",
    "@types/prompts": "^2.0.13",
    "@types/puppeteer": "^5.4.3",
    "@types/rimraf": "^3.0.0",
    "@types/tar": "^4.0.4",
    "@types/update-notifier": "^5.0.0",
    "pptr-testing-library": "^0.6.5",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "typescript": "^4.3.4"
  },
  "files": [
    "dist-cjs",
    "dist-types",
    "types",
    "tsconfig.json",
    "*.js"
  ]
