{
  "name": "modular-scripts",
  "version": "0.12.4",
  "license": "Apache-2.0",
  "bin": {
    "modular": "dist-cjs/cli.js"
  },
  "main": "./dist-cjs/index.js",
  "types": "dist-types/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "modular": "ts-node src/cli.ts",
    "clean": "rimraf build dist-cjs dist-types",
    "prebuild": "yarn clean",
    "build": "yarn build:js && yarn build:ts",
    "build:js": "babel --root-mode upward src --out-dir dist-cjs --extensions .ts --ignore 'src/**/*.test.ts,src/temp/**'",
    "build:ts": "tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
