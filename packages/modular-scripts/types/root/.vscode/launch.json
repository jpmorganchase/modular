{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Tests",
      "type": "node",
      "request": "launch",
      "args": ["${relativeFile}"],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/modular",
      "runtimeArgs": ["test", "--watchAll=false"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    }
  ]
}
